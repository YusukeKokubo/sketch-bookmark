{
    "name" : "HistoryBack",
    "identifier" : "com.phantomtype.sketch.hback",
    "version" : "0.1",
    "description" : "Sketch Plugin for history back and forward.",
    "authorEmail" : "yusuke.kokubo@gmail.com",
    "author" : "Yusuke Kokubo",
    "commands" : [
    {
      "script": "bookmark.js",
      "handlers": {
        "actions": {
          "ArtboardChanged.finish": "onArtboardChanged"
        }
      },
    },
    {
      "script" : "bookmark.js",
      "handler" : "onBookmarkLoad",
      "shortcut" : "option control b",
      "name" : "Load",
      "identifier" : "bookmarkLoad"
    },
    {
      "script" : "bookmark.js",
      "handler" : "onBookmarkSave",
      "shortcut" : "option control d",
      "name" : "Save",
      "identifier" : "bookmarkSave"
    },
    {
      "script" : "bookmark.js",
      "handler" : "onGoBack",
      "shortcut" : "cmd [",
      "name" : "Back",
      "identifier" : "back"
    },
    {
      "script" : "bookmark.js",
      "handler" : "onGoForward",
      "shortcut" : "cmd ]",
      "name" : "Forward",
      "identifier" : "forward"
    },
    {
      "script" : "bookmark.js",
      "handler" : "currentPosition",
      "shortcut" : "cmd control l",
      "name" : "Show Histories",
      "identifier" : "show"
    },
  ],
  "menu": {
    "items": [
      {
        "title": "Bookmark",
        "items": [
          "bookmarkSave",
          "bookmarkLoad"
        ]
      },
      "-",
      "back",
      "forward",
      "show",
    ]
  }
}
