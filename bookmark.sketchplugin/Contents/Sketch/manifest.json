{
    "name" : "HistoryBack",
    "identifier" : "com.phantomtype.sketch.hback",
    "version" : "0.2",
    "description" : "Sketch Plugin for history back and forward.",
    "authorEmail" : "yusuke.kokubo@gmail.com",
    "author" : "Yusuke Kokubo",
    "commands" : [
    {
      "script": "bookmark.js",
      "handlers": {
        "actions": {
          "ArtboardChanged.finish": "onArtboardChanged"
        }
      }
    },
    {
      "script" : "bookmark.js",
      "handler" : "onGoBack",
      "shortcut" : "cmd [",
      "name" : "Back",
      "identifier" : "back"
    },
    {
      "script" : "bookmark.js",
      "handler" : "onGoForward",
      "shortcut" : "cmd ]",
      "name" : "Forward",
      "identifier" : "forward"
    },
    {
      "script" : "bookmark.js",
      "handler" : "currentPosition",
      "shortcut" : "cmd control l",
      "name" : "Show Histories",
      "identifier" : "show"
    }
  ],
  "menu": {
    "items": [
      "back",
      "forward",
      "show"
    ]
  }
}
