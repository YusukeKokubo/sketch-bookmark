{
    "name" : "Bookmark",
    "identifier" : "com.phantomtype.sketchplugin.bookmark",
    "version" : "1.0",
    "description" : "Sketch Plugin for bookmark.",
    "authorEmail" : "yusuke.kokubo@phantomtype.com",
    "author" : "Yusuke Kokubo",
    "commands" : [
    {
      "script": "bookmark.js",
      "handlers": {
        "actions": {
          "ArtboardChanged.finish": "onArtboadChanged"
        }
      },
    },
    {
      "script" : "bookmark.js",
      "handler" : "onRead1",
      "shortcut" : "cmd control j",
      "name" : "read1",
      "identifier" : "read1"
    },
    {
      "script" : "bookmark.js",
      "handler" : "onRead2",
      "shortcut" : "cmd control k",
      "name" : "read2",
      "identifier" : "read2"
    },
    {
      "script" : "bookmark.js",
      "handler" : "onRead3",
      "shortcut" : "cmd control l",
      "name" : "read3",
      "identifier" : "read3"
    },
    {
      "script" : "bookmark.js",
      "handler" : "onSave1",
      "shortcut" : "cmd shift j",
      "name" : "write1",
      "identifier" : "write1"
    },
    {
      "script" : "bookmark.js",
      "handler" : "onSave2",
      "shortcut" : "cmd shift k",
      "name" : "write2",
      "identifier" : "write2"
    },
    {
      "script" : "bookmark.js",
      "handler" : "onSave3",
      "shortcut" : "cmd shift l",
      "name" : "write3",
      "identifier" : "write3"
    },
    {
      "script" : "bookmark.js",
      "handler" : "getBookmarks",
      "shortcut" : "cmd control x",
      "name" : "bookmarks",
      "identifier" : "bookmarks"
    }
  ]
}
