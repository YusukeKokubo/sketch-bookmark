{
    "name" : "Bookmark",
    "identifier" : "com.phantomtype.sketchplugin.bookmark",
    "version" : "1.0",
    "description" : "Sketch Plugin for bookmark.",
    "authorEmail" : "yusuke.kokubo@phantomtype.com",
    "author" : "Yusuke Kokubo",
    "commands" : [
    {
      "script": "bookmark.js",
      "handlers": {
        "actions": {
          "ArtboardChanged.finish": "onArtboadChanged"
        }
      },
    },
    {
      "script" : "bookmark.js",
      "handler" : "onBookmarkLoad",
      "shortcut" : "option control b",
      "name" : "Load",
      "identifier" : "bookmarkLoad"
    },
    {
      "script" : "bookmark.js",
      "handler" : "onBookmarkSave",
      "shortcut" : "option control d",
      "name" : "Save",
      "identifier" : "bookmarkSave"
    },
    {
      "script" : "bookmark.js",
      "handler" : "onGoBack",
      "shortcut" : "cmd control [",
      "name" : "back",
      "identifier" : "back"
    },
    {
      "script" : "bookmark.js",
      "handler" : "onGoForward",
      "shortcut" : "cmd control ]",
      "name" : "forward",
      "identifier" : "forward"
    },
    {
      "script" : "bookmark.js",
      "handler" : "currentPosition",
      "shortcut" : "cmd control l",
      "name" : "c",
      "identifier" : "c"
    },
  ],
  "menu": {
    "items": [
      {
        "title": "Bookmark",
        "items": [
          "bookmarkSave",
          "bookmarkLoad"
        ]
      },
      "back",
      "forward",
      "c"
    ]
  }
}
